{
    "name": "TheRedirector",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Catch and redirect url's",
    "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
    "permissions": ["tabs","webRequest","webRequestBlocking","*://*/*","storage"],
    "background": {
	   "scripts": ["myScript.js"]
	 },
	 "options_page": "options.html"
}